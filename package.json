{
  "name": "apollo-codegen",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.6",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "apollo:codegen": "apollo client:download-schema && apollo client:codegen --target=typescript --outputFlat=./src/apollo.interface.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
